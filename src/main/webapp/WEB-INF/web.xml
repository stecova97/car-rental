<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>main.java.Controller.HomeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>logOut</servlet-name>
    <servlet-class>main.java.Controller.LogOutController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logOut</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>creaModificaUtente</servlet-name>
    <servlet-class>main.java.Controller.CreaModificaUtente</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>creaModificaUtente</servlet-name>
    <url-pattern>/creaModificaUtente</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>parcoAuto</servlet-name>
    <servlet-class>main.java.Controller.ParcoAutoController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>parcoAuto</servlet-name>
    <url-pattern>/parcoAuto</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>creaModificaVeicolo</servlet-name>
    <servlet-class>main.java.Controller.CreaModificaVeicolo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>creaModificaVeicolo</servlet-name>
    <url-pattern>/creaModificaVeicolo</url-pattern>
  </servlet-mapping>

</web-app>
